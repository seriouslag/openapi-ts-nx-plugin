{
  "name": "@seriouslag/nx-openapi-ts-plugin",
  "version": "0.0.31",
  "description": "ðŸš€ Nx plugin for `@hey-api/openapi-ts` codegen.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seriouslag/openapi-ts-nx-plugin.git"
  },
  "license": "MIT",
  "keywords": [
    "codegen",
    "openapi",
    "plugin",
    "nx",
    "swagger"
  ],
  "type": "commonjs",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.cjs.js",
  "types": "./dist/index.cjs.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./package.json": "./package.json"
  },
  "generators": "./generators.json",
  "executors": "./executors.json",
  "dependencies": {
    "@hey-api/json-schema-ref-parser": "^1.0.4",
    "@hey-api/openapi-ts": "^0.66.5",
    "@nx/devkit": "^20.8.0",
    "api-smart-diff": "^1.0.6",
    "latest-version": "^9.0.0",
    "prettier": "^3.5.3",
    "swagger2openapi": "^7.0.8",
    "tslib": "^2.8.1",
    "xcurl": "^2.1.2"
  },
  "devDependencies": {
    "@types/swagger2openapi": "^7.0.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.2"
  },
  "files": [
    "generators.json",
    "executors.json",
    "package.json",
    "dist",
    "src",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
