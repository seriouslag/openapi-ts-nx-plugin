{
  "name": "@seriouslag/nx-openapi-ts-plugin",
  "version": "0.0.3",
  "description": "ðŸš€ Nx plugin for `@hey-api/openapi-ts` codegen.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seriouslag/openapi-ts-nx-plugin.git"
  },
  "license": "MIT",
  "keywords": [
    "codegen",
    "openapi",
    "plugin",
    "nx",
    "swagger"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "scripts": {
    "build": "tsup && pnpm check-exports",
    "check-exports": "attw --pack .",
    "test:watch": "vitest watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.esm.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./package.json": "./package.json"
  },
  "generators": "./generators.json",
  "executors": "./executors.json",
  "dependencies": {
    "@hey-api/json-schema-ref-parser": "^1.0.4",
    "@hey-api/openapi-ts": "^0.66.4",
    "@nx/devkit": "^20.8.0",
    "latest-version": "^9.0.0",
    "openapi-diff": "^0.23.7",
    "tslib": "^2.8.1",
    "xcurl": "^2.1.2"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "tsup": "^8.4.0",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  },
  "files": [
    "package.json",
    "dist",
    "src",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
