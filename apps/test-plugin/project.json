{
  "name": "@test-plugin/test-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/test-plugin/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/test-plugin/dist"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint apps/test-plugin --max-warnings=0",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": []
}
